@model ColorScheme.Models.UserM

@{
    ViewData["Title"] = "Home Page";
    Layout = "_Layout";
}

<div id="demo" class="carousel slide" data-ride="carousel">

    <!-- Indicators -->
    <ul class="carousel-indicators">
        <li data-target="#demo" data-slide-to="0" class="active"></li>
        <li data-target="#demo" data-slide-to="1"></li>
        <li data-target="#demo" data-slide-to="2"></li>
    </ul>

    <!-- The slideshow -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="http://res.freestockphotos.biz/pictures/9/9091-colored-pencils-in-a-circle-isolated-on-a-white-background-pv.jpg" alt="Los Angeles" width="550" height="250">
            <div class="carousel-caption">
                <h3>Color Cord</h3>
                <p>Find your match</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="http://res.freestockphotos.biz/pictures/9/9091-colored-pencils-in-a-circle-isolated-on-a-white-background-pv.jpg" alt="Chicago" width="550" height="250">
            <div class="carousel-caption">
                <h3>Color Cord</h3>
                <p>Find your match</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="http://res.freestockphotos.biz/pictures/9/9091-colored-pencils-in-a-circle-isolated-on-a-white-background-pv.jpg" alt="New York" width="550" height="250">
            <div class="carousel-caption">
                <h3>Color Cord</h3>
                <p>Find your match</p>
            </div>
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="carousel-control-prev" href="#demo" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </a>
    <a class="carousel-control-next" href="#demo" data-slide="next">
        <span class="carousel-control-next-icon"></span>
    </a>
</div>
